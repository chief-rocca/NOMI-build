{
  "expo": {
    "name": "NOMI",
    "slug": "NOMI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nomi-icon.png",
    "scheme": "nomi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/nomi-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nomi.app",
      "icon": "./assets/images/nomi-icon.png",
      "splash": {
        "image": "./assets/images/nomi-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/nomi-icon.png",
      "splash": {
        "image": "./assets/images/nomi-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/nomi-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.nomi.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow NOMI to use FaceID for secure access."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1dc1cbaa-574e-4723-93aa-c1ce5b7377d2"
      }
    }
  }
}